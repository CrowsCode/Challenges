
import java.io.*;

import javax.sound.sampled.AudioInputStream;
import javax.sound.sampled.AudioSystem;
import javax.sound.sampled.Clip;
import javax.sound.sampled.LineUnavailableException;
import javax.sound.sampled.UnsupportedAudioFileException;


class ResultToSpeech {

	public static void play(String result) throws UnsupportedAudioFileException, IOException, LineUnavailableException, InterruptedException {
		
		String[] resultArray = result.split(" ");
		for(int i=0;  i<resultArray.length;  i++) {
			if(resultArray[i].equals("sfr")) { zero();    }
			if(resultArray[i].equals("yak billion")) { oneBillion();    }
			if(resultArray[i].equals("u")) { u();    }
			if(resultArray[i].equals("sad")) { hundred();    }
			if(resultArray[i].equals("hazar")) { thousand();    }
			if(resultArray[i].equals("million")) { million();    }
			if(resultArray[i].equals("yak")) { one();    }
			if(resultArray[i].equals("dw")) { two();    }
			if(resultArray[i].equals("se")) { three();    }
			if(resultArray[i].equals("chwar")) { four();    }
			if(resultArray[i].equals("penj")) { five();    }
			if(resultArray[i].equals("shash")) { six();    }
			if(resultArray[i].equals("7awt")) { seven();    }
			if(resultArray[i].equals("hasht")) { eight();    }
			if(resultArray[i].equals("no")) { nine();    }
			if(resultArray[i].equals("da")) { ten();    }
			if(resultArray[i].equals("yanza")) { eleven();    }
			if(resultArray[i].equals("dwanza")) { twelve();    }
			if(resultArray[i].equals("siyanza")) { thirteen();    }
			if(resultArray[i].equals("chwarda")) { fourteen();    }
			if(resultArray[i].equals("panza")) { fifteen();    }
			if(resultArray[i].equals("shanza")) { sixteen();    }
			if(resultArray[i].equals("7ava")) { seventeen();    }
			if(resultArray[i].equals("hazhda")) { eighteen();    }
			if(resultArray[i].equals("nozda")) { nineteen();    }
			
			if(resultArray[i].equals("bist")) { twenty();    }
			if(resultArray[i].equals("si")) { thirty();    }
			if(resultArray[i].equals("chl")) { fourty();    }
			if(resultArray[i].equals("panja")) { fifty();    }
			if(resultArray[i].equals("shast")) { sixty();    }
			if(resultArray[i].equals("7afta")) { seventy();    }
			if(resultArray[i].equals("hashta")) { eighty();    }
			if(resultArray[i].equals("nawa")) { ninety();    }
			
			
		}
		

		
	}
	public static void u() throws UnsupportedAudioFileException, IOException, LineUnavailableException, InterruptedException {
		File filePath = new File("C:\\Users\\balen\\Desktop\\sounds\\u.wav");
		AudioInputStream audioInputStream = AudioSystem.getAudioInputStream(filePath);
		Clip clip = AudioSystem.getClip();    clip.open(audioInputStream);     clip.start();    	Thread.sleep(500);    }



((((((((((amash SubClass))))))))))) بەڵێن عبدالرحمان  /////بۆ دۆزینەوەی بچوکترین چەنجارەی هاوبەش
import java.util.Scanner;

public class Method {

	int variable;
	int result;
	int times;

	public int find() {
		Scanner input = new Scanner(System.in);
		System.out.print("Enter First Number: ");
		int number1 = input.nextInt();
		System.out.print("Enter Second Number: ");
		int number2 = input.nextInt();
		input.close();
		if (number1 < number2) {
			variable = number1;
		} else {
			variable = number2;
		}

		while (true) {
			result = 0;
			times = times + 1;
			result = times * variable;
			if (((result % number1) == 0) && ((result % number2) == 0)) {
				return result;
			}
		}
	}

}








((((((((((amash Main Class)))))))))))
public class NoChallenge1 {
	public static void main(String[] args) {
		Method m = new Method();
		System.out.println(m.find());
	}
}
